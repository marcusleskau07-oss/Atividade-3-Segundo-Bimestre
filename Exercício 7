programa
{
	
inteiro n1, n2, n3 
	
	funcao inicio()
	{
	escreva("digite um número: ")
	leia(n1)
	escreva("digite outro número: ")
	leia(n2)
	escreva("digite outro número: ")
	leia(n3)
	se(n1 > n2 e n3 > n2)
	{
	escreva(n1, ", ", n3, ", ", n2, ",")
	}
	senao se(n2 > n1 e n1 > n3)
	{
	escreva(n2, ", ", n1, ", ", n3, ",")
	}
	senao se(n3 > n2 e n2 > n1)
	{
	escreva(n3, ", ", n2, ", ", n1, ",")
	}
	}
}
